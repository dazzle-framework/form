{
    "name": "dazzlesoftware/form",
    "type": "library",
    "description": "Dazzle Form Package",
    "keywords": ["dazzle", "framework", "form"],
    "homepage": "https://github.com/dazzle-framework/form",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "dazzlesoftware/language": "^1.0",
        "dazzlesoftware/string": "^1.0",
        "dazzlesoftware/uri": "^1.0",
        "dazzlesoftware/filter": "^1.0",
        "dazzlesoftware/filesystem": "^1.0",
        "dazzlesoftware/registry": "^1.0",
        "dazzlesoftware/utilities": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/database": "^1.0",
        "dazzlesoftware/test": "^1.0",
        "phpunit/phpunit": "^9.0",
        "phpunit/dbunit": "~1.3",
        "squizlabs/php_codesniffer": "1.*"
    },
    "suggest": {
        "dazzlesoftware/database": "Install dazzlesoftware/database if you would like to use the DatabaseConnection field type."
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Form\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Form\\UnitTests\\": "Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "prefer-stable": true
}
